    "@aztlan/jest-config": "^0.4.0",
    "@aztlan/react-helpers": "^0.4.11",
    "@aztlan/storybook-addon-relay": "^0.4.6",
    "@aztlan/stylelint-config": "^0.2.0-alpha.0",
    "@aztlan/typescript-config": "^0.4.0",
    "@aztlan/webpack-config": "^0.4.11",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
