{
  "name": "@aztlan/template-react",
  "version": "0.10.2",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build-storybook": "storybook build",
    "compile:all": "npm run compile:prod && npm run compile:ssr && npm run compile:sitemap",
    "compile:dev": "webpack -c ./webpack.client.dev.mjs",
    "compile:ssr": "webpack -c ./webpack.ssr.renderer.mjs",
    "compile:prod": "webpack -c ./webpack.client.prod.mjs",
    "compile:sitemap": "webpack -c ./webpack.sitemap.renderer.mjs",
    "formatjs:extract": "formatjs extract 'src/**/*.{ts,tsx}' --out-file src/extracted-locales/en.json --format simple",
    "formatjs:compile:en": "formatjs compile src/extracted-locales/en.json --ast --out-file src/locales/en.json --format simple",
    "formatjs:compile:es": "formatjs compile src/extracted-locales/es.json --ast --out-file src/locales/es.json --format simple",
    "relay": "relay-compiler",
    "relay:debug": "relay-compiler --output debug",
    "schema:development": "fetch-schema",
    "schema:production": "fetch-schema --ref production",
    "start": "npm run start:dev",
    "start:dev": "webpack serve -c ./webpack.client.dev.mjs",
    "start:prod": "webpack serve -c ./webpack.client.prod.mjs",
    "start:sitemap": "webpack -c ./webpack.sitemap.server.mjs",
    "start:ssr": "webpack -c ./webpack.ssr.server.mjs",
    "storybook": "storybook dev -p 6007 --no-open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:tsc": "tsc",
    "lint:eslint": "eslint --ext .cjs,.mjs,.js,.tsx,.ts ./ --quiet",
    "lint:stylelint": "stylelint 'src/**/*.scss' --rd --rdd --risd --color --formatter verbose",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:stylelint": "npm run lint:stylelint -- --fix",
    "test:jest": "jest --verbose",
    "test:jest:old": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --verbose",
    "test:jest:update": "npm run test:jest -- --updateSnapshot"
  },
  "author": "",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@aztlan/babel-preset-react": "^0.10.0",
    "@aztlan/bem": "^0.6.0",
    "@aztlan/eslint-config-base": "^0.10.0",
    "@aztlan/jest-config": "^0.10.0",
    "@aztlan/react-helpers": "^0.10.1",
    "@aztlan/storybook-addon-relay": "^0.10.0",
    "@aztlan/stylelint-config": "^0.6.0",
    "@aztlan/typescript-config": "^0.10.0",
    "@aztlan/webpack-config": "^0.10.0",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@formatjs/cli": "^6.2.7",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-storyshots": "^7.6.10",
    "@storybook/addon-themes": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-webpack5": "^7.6.10",
    "@storybook/testing-library": "^0.2.2",
    "babel-plugin-formatjs": "^10.5.13",
    "babel-plugin-relay": "^16.2.0",
    "css-loader": "^6.9.1",
    "eslint": "^8.56.0",
    "graphql": "^16.8.1",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss-loader": "^8.0.0",
    "relay-compiler": "^16.2.0",
    "sass-loader": "^14.0.0",
    "storybook": "^7.6.10",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@aztlan/assets": "^0.10.0",
    "@aztlan/base-styles": "^0.10.0",
    "@aztlan/react-hooks": "^0.10.2",
    "@aztlan/react-ssr": "^0.10.2",
    "@aztlan/storybook-config": "^0.10.0",
    "@aztlan/storybook-helpers": "^0.10.0",
    "@aztlan/ui": "^0.10.2",
    "@loadable/component": "^5.16.3",
    "@loadable/server": "^5.16.2",
    "date-fns": "^3.3.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-intl": "^6.6.2",
    "react-relay": "^16.2.0",
    "react-relay-network-modern": "^6.2.2",
    "react-router-dom": "^5.3.4",
    "relay-runtime": "^16.2.0"
  },
  "relay": {
    "src": "./src",
    "schema": "./schema.graphql",
    "language": "typescript"
  },
  "babel": {
    "presets": [
      "@aztlan/react"
    ],
    "plugins": [
      "relay",
      "formatjs"
    ]
  },
  "stylelint": {
    "extends": "@aztlan/stylelint-config"
  },
  "eslintConfig": {
    "extends": "@aztlan/eslint-config-base"
  }
}
