{
  "name": "@aztlan/react-helpers",
  "version": "0.4.11",
  "description": "",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "exports":{
    ".":{
      "import":"dist/esm/index.mjs",
      "require":"dist/cjs/index.cjs"
    },
    "./ssr":{
      "import":"dist/esm/ssr/index.mjs",
      "require":"dist/cjs/ssr/index.cjs"
    },
    "./relay":{
      "import":"dist/esm/relay/index.mjs",
      "require":"dist/cjs/relay/index.cjs"
    }
  },
  "bin": {
    "fetch-schema": "./scripts/fetchSchema.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:tsc": "tsc",
    "test:eslint": "eslint --ext .js,.tsx,.ts src/ && eslint --ext .js,.tsx,.ts stories/",
    "build:cjs": "node ./build.mjs --format cjs",
    "build:cjs:watch": "node ./build.mjs --format cjs --watch",
    "build:esm": "node ./build.mjs --format esm",
    "build:esm:watch": "node ./build.mjs --format esm --watch",
    "compile": "npm run build:cjs && npm run build:esm",
    "postinstall": "npm run compile"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aztlan/build-tools": "^0.4.11",
    "@aztlan/eslint-config-base": "^0.4.0",
    "@aztlan/typescript-config": "^0.4.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "react-relay-network-modern": "^6.2.2",
    "relay-runtime": "^16.0.0"
  },
  "eslintConfig": {
    "extends": "@aztlan/eslint-config-base"
  }
}
