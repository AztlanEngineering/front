{
  "name": "@aztlan/ui",
  "version": "0.13.3",
  "description": "",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "build-with-babel --format cjs",
    "build:esm": "build-with-babel --format esm",
    "build:types": "tsc --emitDeclarationOnly",
    "build-storybook": "storybook build",
    "relay": "relay-compiler",
    "relay:debug": "relay-compiler --output debug",
    "schema:development": "fetch-schema",
    "schema:production": "fetch-schema --ref production",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006 --debug --no-open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:tsc": "tsc --noEmit",
    "lint:tsc:verbose": "tsc --noEmit --noErrorTruncation",
    "lint:eslint": "eslint --ext .cjs,.mjs,.js,.tsx,.ts ./ --quiet",
    "lint:stylelint": "stylelint 'src/**/*.scss' --rd --rdd --risd --color --formatter verbose",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:stylelint": "npm run lint:stylelint -- --fix",
    "test:jest": "jest --verbose",
    "test:jest:old": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --verbose",
    "test:jest:update": "npm run test:jest -- --updateSnapshot"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aztlan/assets": "^0.13.0",
    "@aztlan/babel-preset-react": "^0.13.0",
    "@aztlan/eslint-config-base": "^0.13.0",
    "@aztlan/jest-config": "^0.13.0",
    "@aztlan/react-scripts": "^0.13.3",
    "@aztlan/storybook-config": "^0.13.0",
    "@aztlan/stylelint-config": "^0.9.0",
    "@aztlan/typescript-config": "^0.13.0",
    "@aztlan/webpack-config": "^0.13.0",
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-typescript": "^7.23.6",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-essentials": "^7.6.11",
    "@storybook/addon-interactions": "^7.6.11",
    "@storybook/addon-links": "^7.6.11",
    "@storybook/addon-storyshots": "^7.6.11",
    "@storybook/addon-themes": "^7.6.11",
    "@storybook/blocks": "^7.6.11",
    "@storybook/react-webpack5": "^7.6.11",
    "@storybook/testing-library": "^0.2.2",
    "@stylistic/eslint-plugin": "^1.5.4",
    "@types/jest": "^29.5.11",
    "@types/prop-types": "^15.7.11",
    "babel-jest": "^29.7.0",
    "babel-plugin-relay": "^16.2.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "storybook": "^7.6.11",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2"
  },
  "dependencies": {
    "@aztlan/base-styles": "^0.13.0",
    "@aztlan/bem": "^0.9.0",
    "@aztlan/react-hooks": "^0.13.3",
    "@aztlan/react-relay": "^0.13.3",
    "@aztlan/storybook-addon-relay": "^0.13.0",
    "@storybook/react": "^7.6.11",
    "babel-plugin-formatjs": "^10.5.13",
    "downshift": "^8.3.1",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-intl": "^6.6.2",
    "react-relay": "^16.2.0",
    "react-router-dom": "^5.3.4",
    "relay-compiler": "^16.2.0",
    "relay-runtime": "^16.2.0"
  },
  "relay": {
    "src": "./src",
    "schema": "./schema.graphql",
    "language": "typescript",
    "eagerEsModules": true
  },
  "babel": {
    "presets": [
      "@aztlan/react"
    ],
    "plugins": [
      "relay",
      "formatjs"
    ]
  },
  "stylelint": {
    "extends": "@aztlan/stylelint-config"
  },
  "eslintConfig": {
    "extends": "@aztlan/eslint-config-base"
  },
  "buildConfig": {
    "entryPoints": [
      "./src/ui/**/*.{ts,tsx,js,jsx}"
    ],
    "ignore": [
      "./**/*.stories.tsx"
    ],
    "copyfiles": [
      "src/ui/*.scss",
      "src/ui/**/*.scss",
      "src/ui/**/*.css"
    ],
    "up": 2
  }
}
