{
  "name": "@aztlan/ui",
  "version": "2.0.1",
  "description": "",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "build-with-babel --format cjs",
    "build:esm": "build-with-babel --format esm",
    "build:types": "tsc --emitDeclarationOnly",
    "build-storybook": "storybook build",
    "relay": "relay-compiler",
    "relay:debug": "relay-compiler --output debug",
    "schema:development": "fetch-schema",
    "schema:production": "fetch-schema --ref production",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006 --debug --no-open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:tsc": "tsc --noEmit",
    "lint:tsc:verbose": "tsc --noEmit --noErrorTruncation",
    "lint:eslint": "eslint --ext .cjs,.mjs,.js,.tsx,.ts ./ --quiet",
    "lint:stylelint": "stylelint 'src/**/*.scss' --rd --rdd --risd --color --formatter verbose",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:stylelint": "npm run lint:stylelint -- --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aztlan/assets": "^2.0.0",
    "@aztlan/babel-preset-react-relay-app": "^2.0.0",
    "@aztlan/eslint-config-base": "^2.0.0",
    "@aztlan/react-scripts": "^2.0.0",
    "@aztlan/storybook-config": "^2.0.0",
    "@aztlan/stylelint-config": "^0.9.0",
    "@aztlan/typescript-config": "^2.0.0",
    "@aztlan/webpack-config": "^2.0.0",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-storyshots": "^7.6.16",
    "@storybook/addon-themes": "^7.6.16",
    "@storybook/blocks": "^7.6.16",
    "@storybook/react-webpack5": "^7.6.16",
    "@storybook/testing-library": "^0.2.2",
    "@stylistic/eslint-plugin": "^1.6.2",
    "@types/prop-types": "^15.7.11",
    "postcss-scss": "^4.0.9",
    "sass": "^1.71.0",
    "storybook": "^7.6.16",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aztlan/base-styles": "^2.0.0",
    "@aztlan/bem": "^0.9.6",
    "@aztlan/react-hooks": "^2.0.0",
    "@aztlan/react-relay": "^2.0.0",
    "@aztlan/storybook-addon-relay": "^2.0.0",
    "@storybook/react": "^7.6.16",
    "downshift": "^8.3.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-intl": "^6.6.2",
    "react-relay": "^16.2.0",
    "react-router-dom": "^5.3.4",
    "relay-compiler": "^16.2.0",
    "relay-runtime": "^16.2.0"
  },
  "relay": {
    "src": "./src",
    "schema": "./schema.graphql",
    "language": "typescript",
    "eagerEsModules": true
  },
  "babel": {
    "presets": [
      "@aztlan/react-relay-app"
    ]
  },
  "stylelint": {
    "extends": "@aztlan/stylelint-config"
  },
  "eslintConfig": {
    "extends": "@aztlan/eslint-config-base"
  },
  "buildConfig": {
    "entryPoints": [
      "./src/ui/**/*.{ts,tsx,js,jsx}"
    ],
    "ignore": [
      "./**/*.stories.tsx"
    ],
    "copyfiles": [
      "src/ui/*.scss",
      "src/ui/**/*.scss",
      "src/ui/**/*.css"
    ],
    "up": 2
  }
}
