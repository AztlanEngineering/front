{
  "name": "@aztlan/ui",
  "version": "0.6.0",
  "description": "",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build-storybook": "storybook build",
    "build:cjs": "node ./build.mjs --format cjs",
    "build:cjs:watch": "node ./build.mjs --format cjs --watch",
    "build:esm": "node ./build.mjs --format esm",
    "build:esm:watch": "node ./build.mjs --format esm --watch",
    "relay": "relay-compiler",
    "relay:debug": "relay-compiler --output debug",
    "schema:development": "fetch-schema",
    "schema:production": "fetch-schema --ref production",
    "prepare": "npm run build",
    "storybook": "storybook dev -p 6006 --debug",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:tsc": "tsc",
    "lint:eslint": "eslint --ext .cjs,.mjs,.js,.tsx,.ts ./ --quiet",
    "lint:stylelint": "stylelint 'src/**/*.scss' --rd --rdd --risd --color --formatter verbose",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:stylelint": "npm run lint:stylelint -- --fix",
    "test:jest": "jest --verbose",
    "test:jest:old": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --verbose",
    "test:jest:update": "npm run test:jest -- --updateSnapshot"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aztlan/assets": "^0.6.0",
    "@aztlan/babel-preset-react": "^0.6.0",
    "@aztlan/build-tools": "^0.6.0",
    "@aztlan/eslint-config-base": "^0.6.0",
    "@aztlan/jest-config": "^0.6.0",
    "@aztlan/storybook-config": "^0.6.0",
    "@aztlan/stylelint-config": "^0.3.0",
    "@aztlan/typescript-config": "^0.6.0",
    "@aztlan/webpack-config": "^0.6.0",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-essentials": "^7.6.5",
    "@storybook/addon-interactions": "^7.6.5",
    "@storybook/addon-links": "^7.6.5",
    "@storybook/addon-storyshots": "^7.6.5",
    "@storybook/addon-themes": "^7.6.5",
    "@storybook/blocks": "^7.6.5",
    "@storybook/react": "^7.6.5",
    "@storybook/react-webpack5": "^7.6.5",
    "@storybook/testing-library": "^0.2.2",
    "@stylistic/eslint-plugin": "^1.5.1",
    "@types/jest": "^29.5.11",
    "@types/prop-types": "^15.7.11",
    "babel-jest": "^29.7.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "storybook": "^7.6.5",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2"
  },
  "dependencies": {
    "@aztlan/base-styles": "^0.6.0",
    "@aztlan/bem": "^0.3.4",
    "@aztlan/react-helpers": "^0.6.0",
    "@aztlan/storybook-addon-relay": "^0.6.0",
    "babel-plugin-formatjs": "^10.5.10",
    "babel-plugin-relay": "^16.1.0",
    "formik": "^2.4.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^16.1.0",
    "relay-compiler": "^16.1.0",
    "relay-runtime": "^16.1.0"
  },
  "relay": {
    "src": "./src",
    "schema": "./schema.graphql",
    "language": "typescript"
  },
  "babel": {
    "presets": [
      "@aztlan/react"
    ],
    "plugins": [
      "relay",
      "formatjs"
    ]
  },
  "stylelint": {
    "extends": "@aztlan/stylelint-config"
  },
  "eslintConfig": {
    "extends": "@aztlan/eslint-config-base"
  }
}
