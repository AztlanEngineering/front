{
  "name": "@aztlan/storybook-helpers",
  "version": "0.12.0",
  "description": "",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:tsc": "tsc",
    "test:eslint": "eslint --ext .mjs,.js,.tsx,.ts ./",
    "test:eslint:fix": "npm run test:eslint -- --fix",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "build-with-babel --format cjs",
    "build:esm": "build-with-babel --format esm",
    "build:types": "tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aztlan/eslint-config-base": "^0.12.0",
    "@aztlan/react-scripts": "^0.12.0",
    "@aztlan/typescript-config": "^0.12.0",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": "@aztlan/eslint-config-base"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^16.2.0",
    "relay-test-utils": "^16.2.0"
  },
  "buildConfig": {
    "entryPoints": [
      "./src/**/*.{ts,tsx,js,jsx}"
    ],
    "up": 1
  }
}
