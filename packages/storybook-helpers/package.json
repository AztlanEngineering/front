{
  "name": "@aztlan/storybook-helpers",
  "version": "0.4.3",
  "description": "",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:tsc": "tsc",
    "test:eslint": "eslint --ext .mjs,.js,.tsx,.ts ./",
    "test:eslint:fix": "npm run test:eslint -- --fix",
    "build:cjs": "node ./build.mjs --format cjs",
    "build:cjs:watch": "node ./build.mjs --format cjs --watch",
    "build:esm": "node ./build.mjs --format esm",
    "build:esm:watch": "node ./build.mjs --format esm --watch",
    "compile": "npm run build:cjs && npm run build:esm",
    "postinstall": "npm run compile"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aztlan/build-tools": "^0.4.0",
    "@aztlan/eslint-config-base": "^0.4.0",
    "@aztlan/typescript-config": "^0.4.0",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "extends": "@aztlan/eslint-config-base"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^16.0.0",
    "relay-test-utils": "^16.0.0"
  }
}
