{
  "name": "@aztlan/design-system",
  "version": "0.4.5",
  "description": "",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build-storybook": "storybook build",
    "build:cjs": "node ./build.mjs --format cjs",
    "build:cjs:watch": "node ./build.mjs --format cjs --watch",
    "build:esm": "node ./build.mjs --format esm",
    "build:esm:watch": "node ./build.mjs --format esm --watch",
    "postinstall": "npm run build",
    "storybook": "storybook dev -p 6006 --debug",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:eslint": "eslint --ext .mjs,.js,.tsx,.ts ./",
    "test:eslint:fix": "npm run test:eslint -- --fix",
    "test:jest": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --verbose",
    "test:jest:update": "npm run test:jest -- --updateSnapshot",
    "test:stylelint": "stylelint 'src/**/*.scss' --rd --rdd --risd --color --formatter verbose",
    "test:stylelint:fix": "npm run test:stylelint -- --fix",
    "test:tsc": "tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aztlan/assets": "^0.4.0",
    "@aztlan/babel-preset-react": "^0.4.0",
    "@aztlan/build-tools": "^0.4.5",
    "@aztlan/eslint-config-base": "^0.4.0",
    "@aztlan/jest-config": "^0.4.0",
    "@aztlan/storybook-config": "^0.4.3",
    "@aztlan/stylelint-config": "^0.2.0-alpha.0",
    "@aztlan/typescript-config": "^0.4.0",
    "@aztlan/webpack-config": "^0.4.0",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-storyshots": "^7.5.3",
    "@storybook/addon-themes": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/jest": "^29.5.8",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.1.2",
    "jest-environment-jsdom": "^29.7.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "storybook": "^7.5.3",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2"
  },
  "dependencies": {
    "@aztlan/base-styles": "^0.4.3",
    "@aztlan/bem": "^0.2.5",
    "@aztlan/react-helpers": "^0.4.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "babel": {
    "presets": [
      "@aztlan/react"
    ],
    "plugins": [
      "relay"
    ]
  },
  "stylelint": {
    "extends": "@aztlan/stylelint-config"
  },
  "eslintConfig": {
    "extends": "@aztlan/eslint-config-base"
  }
}
