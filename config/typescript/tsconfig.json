{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "incremental": false,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es2020", "dom"],
    "noImplicitAny": false,
    "baseUrl":"src",
    "skipLibCheck": true,
    "target": "ES2020",
    "moduleResolution": "nodenext",
    "module":"NodeNext",
    "noEmit":true
  }
}
