{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es2022", "dom"],
    "module":"NodeNext",
    "moduleResolution": "nodenext",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "target": "ES2022"
  }
}
